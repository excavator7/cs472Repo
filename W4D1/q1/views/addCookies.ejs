<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8"/>
        <title>Add Cookies</title>
    </head>
    <body>
        <div id="container">
            <h1>Add Cookie</h1>
            <form action="/" method="post">
                <label>Key:</label>
                <input type="text" name="key">
                <label>Value:</label>
                <input type="text" name="value">
                <div>
                    <input type="submit">
                </div>
            </form>

            <div>
                <table>
                    <th>Key</th>
                    <th>Value</th>
                    <%for(const cl of cookiesList){%>
                        <tr>
                            <td><%= cl.key%></td>
                            <td><%= cl.value%></td>
                        </tr>
                    <%}%>
                </table>
            </div>
        </div>
    </body>
</html>