<!DOCTYPE>
<html>
    <head>
        <title>Shopping Cart</title>
    </head>
    <body id="carId">
        <h1>Shopping Cart</h1>
        <div>
            <% for(let i of cartItems) { %> 
                <div><label>Name:</label><%=i.name%></div>
                <div><label>Quantity:</label><%=i.quantity%></div>
                <div><label>Price:</label><%=i.price%></div>
             <% } %>
        </div>
         <div>
            <a href="/">Back</a>
         </div>
    </body>
</html>